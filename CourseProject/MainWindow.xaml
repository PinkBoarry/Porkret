        Title="{Binding Content.Title, ElementName=navigationFrame}" Height="800" Width="1150" MinWidth="1150" MinHeight="800" Icon="/my2.png">
    <Frame x:Name="navigationFrame" NavigationUIVisibility="Hidden"/>
</Window>
